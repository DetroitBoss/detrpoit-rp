ify('Непредвиденная ошибка', 'error');\n    return player.user.setGui(null);\n  }\n  let dressId = item.advancedNumber;\n  let variation = 0;\n  if (dressId >= _cloth.CLOTH_VARIATION_ID_MULTIPLER) {\n    variation = Math.floor(dressId / _cloth.CLOTH_VARIATION_ID_MULTIPLER);\n    dressId = dressId % _cloth.CLOTH_VARIATION_ID_MULTIPLER;\n  }\n  const dressConfig = _customization.dress.get(dressId);\n  if (dressConfig.male !== player.user.is_male) {\n    return player.notify(`Это ${dressConfig.male ? 'мужская' : 'женская'} одежда`, 'error');\n  }\n  dressConfig.data[variation].forEach(data => {\n    player.user.setDress(data.component, data.drawable, data.texture);\n  });\n});\n_custom.CustomEvent.registerClient('market:closed', player => {\n  player.user.reloadSkin();\n});"},"sourceMaps":{"js":{"mappings":[{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":3,"column":0}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":3,"column":4}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":3,"column":11}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":3,"column":14}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":1,"column":0},"generated":{"line":3,"column":21}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":2,"column":0},"generated":{"line":4,"column":0}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":2,"column":0},"generated":{"line":4,"column":4}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":2,"column":0},"generated":{"line":4,"column":14}},{"source":"modules/market/logic/clothesPreview.ts","name":null,"original":{"line":2,"column":0},"generated":{"line":4,"column":17}},