ket/RentTimer.ts","name":null,"original":{"line":60,"column":12},"generated":{"line":52,"column":12}},{"source":"modules/market/RentTimer.ts","name":null,"original":{"line":60,"column":20},"generated":{"line":52,"column":20}},{"source":"modules/market/RentTimer.ts","name":null,"original":{"line":60,"column":22},"generated":{"line":52,"column":22}},{"source":"modules/market/RentTimer.ts","name":null,"original":{"line":60,"column":26},"generated":{"line":52,"column":26}},{"source":"modules/market/RentTimer.ts","name":null,"original":{"line":60,"column":27},"generated":{"line":52,"column":27}}],"sources":{"modules/market/RentTimer.ts":"export class RentTimer {\n    public timeEndHandler: () => void\n\n    public constructor(\n        private rentTime: number\n    ) {\n        timersPool.push({\n            timer: this,\n            destroyHandler: this.destroyByTime.bind(this),\n            decreaseTickHandler: this.decreaseTick.bind(this)\n        });\n    }\n\n    public get timeLeftS() {\n        return this.rentTime;\n    }\n\n    public getMaxExpandTime(): number {\n        return 0;\n    }\n\n    public expandTime(addTimeS: number) {\n        this.rentTime += addTimeS;\n    }\n\n    public destroy() {\n        const idx = timersPool.findIndex(obj => obj.timer === this);\n        if (idx === -1) {\n            return;\n        }\n\n        timersPool.splice(idx);\n    }\n\n    private decreaseTick() {\n        this.rentTime--;\n    }\n\n    private destroyByTime() {\n        if (this.timeEndHandler) {\n            this.timeEndHandler();\n        }\n\n        this.destroy();\n    }\n}\n\nconst timersPool: { timer: RentTimer, destroyHandler: () => void, decreaseTickHandler: () => void }[] = [];\n\nfunction rentTick() {\n    timersPool.forEach(obj => {\n        obj.decreaseTickHandler();\n\n        if (obj.timer.timeLeftS <= 0) {\n            obj.destroyHandler();\n        }\n    })\n}\n\nsetInterval(rentTick, 1000);"},"lineCount":null}},"error":null,"hash":"660097ab