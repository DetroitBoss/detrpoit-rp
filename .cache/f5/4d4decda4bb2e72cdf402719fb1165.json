rval(() => {\n    if (hospitalTime === null) return;\n\n    if (hospitalTime - 1 <= 0) {\n        hospitalTime = null;\n        user.notify(`Вы успешно прошли курс лечения`, 'success');\n    }else{\n        hospitalTime -= 1;\n    }\n}, 1000);"},"lineCount":null}},"error":null,"hash":"91be285353fb03e75f0e45c27779fa71","cacheData":{"env":{}}}