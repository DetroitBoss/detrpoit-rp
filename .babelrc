{
    "presets": [
        [
            "@babel/preset-env", {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {}, "proposal-class-properties"],
        ["@babel/plugin-syntax-dynamic-import", {}, "syntax-dynamic-import"],
        ["@babel/plugin-proposal-function-bind", {}, "proposal-function-bind"]
    ]
}